{
  "name": "@reshaped/blog",
  "version": "0.4.0",
  "description": "Personal blog starter kit built on top of Next.js and Reshaped",
  "license": "MIT",
  "keywords": [
    "react",
    "nextjs",
    "blog",
    "reshaped",
    "personal website",
    "static site generator"
  ],
  "bugs": {
    "url": "https://github.com/formaat-design/reshaped-blog-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formaat-design/reshaped-blog-starter.git"
  },
  "bin": {
    "reshaped-blog": "bin/cli.mjs"
  },
  "scripts": {
    "clone": "node --env-file=.env.local prebuild.mjs",
    "clone:dev": "nodemon --env-file=.env.local prebuild.mjs",
    "predev": "npm run clone",
    "dev": "next dev",
    "prebuild": "npm run clone",
    "build": "next build",
    "build:themes": "reshaped theming --output ./src/themes",
    "start": "next start",
    "lint": "next lint",
    "release": "yarn publish --access public",
    "example:dev": "yarn --cwd ./example dev",
    "example:build": "yarn --cwd ./example build",
    "example:start": "yarn --cwd ./example start"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@octokit/rest": "^21.0.1",
    "@types/mdx": "^2.0.11",
    "@types/node": "20.11.19",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "axios": "^1.7.4",
    "commander": "12.0.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "execa": "8.0.1",
    "fs-extra": "11.2.0",
    "gray-matter": "^4.0.3",
    "next": "14.1.0",
    "next-mdx-remote": "^4.4.1",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "remark": "^15.0.1",
    "remark-defsplit": "^5.0.0",
    "remark-images": "^4.0.0",
    "reshaped": "2.9.5",
    "shiki": "1.1.3",
    "slugify": "^1.6.6",
    "typescript": "5.3.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "resolve-tspaths": "^0.8.18"
  }
}
